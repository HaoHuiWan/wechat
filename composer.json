{
    "name": "zongu/laravel-wechat",
    "description": "微信 SDK for Laravel",
    "keywords": ["wechat", "weixin","laravel", "sdk"],
    "license": "MIT",
    "authors": [
        {
            "name": "zongu",
            "email": "zongu@gmail.com"
        }
    ],
    "require": {
        "illuminate/container": "^5.1 || ^6.0 || ^7.0 || ^8.0"
    },
    "require-dev": {
        "laravel/framework": "^8.5",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "Zongu\\LaravelWeChat\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zongu\\LaravelWeChat\\ServiceProvider"
            ],
            "aliases": {
                "EasyWeChat": "Zongu\\LaravelWeChat\\Facade"
            }
        }
    }
}
